mixin link(title, href, text, target)
  - if (typeof(text) === 'undefined') text = title
  - if (typeof(href) === 'undefined') href = '#'
  a(href=href, title=title, target=target)&attributes(attributes)
    if block
      block
    else
      !=text

mixin img-link(objLink)
  +link(objLink.title, objLink.href, objLink.text, objLink.target)(class=objLink.nameclass)
    +img(objLink.alt, objLink.src)

